<%- include("./partials/header") -%>
    <h1>Compose</h1>
    <form class="item" action="/compose" method="post">
        <div class="form-group">
            <label>Title</label>
            <input class="form-control" type="text" name="postTitle" placeholder="" autocomplete="off">
            <!-- autocomplete 자동완성 제거 -->
            <label>Post</label>
            <textarea class="form-control" name="postBody" rows="8" cols="30"></textarea>
            <button class="btn btn-primary hey" type="submit" name="button">Publish</button>
        </div>

    </form>
    <%- include("./partials/footer") -%>